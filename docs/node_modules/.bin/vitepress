#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xuzhidan/work/fluth/node_modules/.pnpm/vitepress@1.3.4_@algolia+client-search@5.2.5_sass@1.79.3_search-insights@2.17.1/node_modules/vitepress/bin/node_modules:/Users/xuzhidan/work/fluth/node_modules/.pnpm/vitepress@1.3.4_@algolia+client-search@5.2.5_sass@1.79.3_search-insights@2.17.1/node_modules/vitepress/node_modules:/Users/xuzhidan/work/fluth/node_modules/.pnpm/vitepress@1.3.4_@algolia+client-search@5.2.5_sass@1.79.3_search-insights@2.17.1/node_modules:/Users/xuzhidan/work/fluth/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/xuzhidan/work/fluth/node_modules/.pnpm/vitepress@1.3.4_@algolia+client-search@5.2.5_sass@1.79.3_search-insights@2.17.1/node_modules/vitepress/bin/node_modules:/Users/xuzhidan/work/fluth/node_modules/.pnpm/vitepress@1.3.4_@algolia+client-search@5.2.5_sass@1.79.3_search-insights@2.17.1/node_modules/vitepress/node_modules:/Users/xuzhidan/work/fluth/node_modules/.pnpm/vitepress@1.3.4_@algolia+client-search@5.2.5_sass@1.79.3_search-insights@2.17.1/node_modules:/Users/xuzhidan/work/fluth/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/vitepress@1.3.4_@algolia+client-search@5.2.5_sass@1.79.3_search-insights@2.17.1/node_modules/vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/vitepress@1.3.4_@algolia+client-search@5.2.5_sass@1.79.3_search-insights@2.17.1/node_modules/vitepress/bin/vitepress.js" "$@"
fi
